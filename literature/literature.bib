pages = {78--99},
pmid = {26343675},
publisher = {IAgrE},
title = {{Assessing the ventilation effectiveness of naturally ventilated livestock buildings under wind dominated conditions using computational fluid dynamics}},
url = {http://dx.doi.org/10.1016/j.biosystemseng.2009.02.007},
volume = {103},
year = {2009}
}
