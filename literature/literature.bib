isbn = {0360-1323},
issn = {03601323},
journal = {Building and Environment},
keywords = {Air flow,Building,Computational Fluid Dynamics (CFD),Natural ventilation,Parametric study,Sensitivity study},
number = {0},
pages = {34--48},
publisher = {Elsevier Ltd},
title = {{CFD simulation of cross-ventilation for a generic isolated building: Impact of computational parameters}},
url = {http://dx.doi.org/10.1016/j.buildenv.2012.01.004},
volume = {53},
year = {2012}
}
