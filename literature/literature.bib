number = {8},
pages = {2021--2027},
publisher = {Elsevier B.V.},
title = {{Mean age of air in a naturally ventilated office: Experimental data and simulations}},
url = {http://dx.doi.org/10.1016/j.enbuild.2011.04.015},
volume = {43},
year = {2011}
}
