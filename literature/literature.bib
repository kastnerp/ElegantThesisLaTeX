keywords = {Building aerodynamics,CFD,Natural ventilation,Urban physics,Urban wind flow,Ventilation efficiency},
pages = {152--166},
title = {{CFD simulation of outdoor ventilation of generic urban configurations with different urban densities and equal and unequal street widths}},
volume = {92},
year = {2015}
}
