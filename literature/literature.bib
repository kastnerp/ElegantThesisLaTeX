journal = {Landscape and Urban Planning},
keywords = {Frontal area density,Urban planning,Urban roughness,Urban ventilation},
number = {1},
pages = {59--74},
publisher = {Elsevier B.V.},
title = {{Improving the wind environment in high-density cities by understanding urban morphology and surface roughness: A study in Hong Kong}},
url = {http://dx.doi.org/10.1016/j.landurbplan.2011.01.004},
volume = {101},
year = {2011}
}
@article{NgelPadilla-Marcos2016,
abstract = {Outdoor spaces are air volumes between buildings that are open to atmospheric air. Surrounding build-ings hinder and alter airflow, which affect the exchange process and its quality. Given that this outdoor air is captured to ventilate a building's interior, the geometric characteristics and wind exposure of these outdoor spaces are vital to optimising these processes. One of the parameters under study is the variable wind speed. The goal of this article is to ascertain whether different wind velocities affect airflow patterns in these spaces. To this end, eight archetypal shape/volumetric cases of urban buildings are examined from an air exchange efficiency perspective. Practical application: The result is described by a mathematical expression that links the exchange efficiency with a range of wind velocities (0.75 6.00 m/s), which confirmed that this wind velocity range implies variations of only AE1.0{\%} in interior courtyards, AE1.6{\%} in adjacent exterior spaces between buildings, and AE2.4{\%} in the considered control volume. Highlights . Impact of wind velocity on air quality and exterior ventilation efficiency; . Application of the concept of age of outdoor air on turbulence models; . Characterisation of interior air located in confined outdoor spaces; . Behavioural analysis of outdoor air in eight architectural types with outdoor space; . Ventilation efficiency equation according to variable wind velocity.},
author = {{Ngel Padilla-Marcos}, Miguel and {Feij{\'{o}}-Mu{\~{n}} Oz}, Jes{\'{u}} S and Meiss, Alberto},
doi = {10.1177/0143624415596441},
