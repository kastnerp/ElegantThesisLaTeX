issn = {1476-8259},
journal = {Computer methods in biomechanics and biomedical engineering},
keywords = {Arteries,Arteries: physiology,Blood Circulation,Computer Simulation,Heart Rate,Humans},
number = {5},
pages = {625--640},
pmid = {20140798},
title = {{Outflow boundary conditions for 3D simulations of non-periodic blood flow and pressure fields in deformable arteries.}},
