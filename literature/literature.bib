journal = {Energy and Buildings},
keywords = {Buoyancy,CFD,Indoor ventilation,Urban environment,Wind tower},
number = {7},
pages = {1784--1791},
publisher = {Elsevier B.V.},
title = {{A study of wind and buoyancy driven flows through commercial wind towers}},
url = {http://dx.doi.org/10.1016/j.enbuild.2011.03.022},
volume = {43},
year = {2011}
}
