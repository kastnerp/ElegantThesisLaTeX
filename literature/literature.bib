isbn = {9781467304269},
issn = {03787788},
journal = {BEIAC 2012 - 2012 IEEE Business, Engineering and Industrial Applications Colloquium},
keywords = {air change per hour (ACH),air movement,hot and humid climatic conditions,wind-induced natural ventilation tower},
pages = {178--183},
publisher = {Elsevier B.V.},
title = {{Case study of wind-induced natural ventilation tower in hot and humid climatic conditions}},
url = {http://dx.doi.org/10.1016/j.enbuild.2012.05.016},
volume = {52},
