keywords = {Computational fluid dynamics (CFD),Experimental validation,correlation,cross-ventilation,loggia effect,parametric analysis},
pages = {131--142},
publisher = {Elsevier Ltd},
title = {{Numerical simulation of wind-driven natural ventilation: effects of loggia and facade porosity on air change rate}},
url = {http://www.sciencedirect.com/science/article/pii/S0360132316301044},
volume = {106},
year = {2016}
}
