publisher = {Elsevier B.V.},
title = {{An open-source library for the numerical modeling of mass-transfer in solid oxide fuel cells}},
url = {http://dx.doi.org/10.1016/j.cpc.2011.08.003},
volume = {183},
year = {2012}
}
@article{Novoselac2003,
abstract = {Recently, indoor air quality (IAQ) became an important issue and as a result researchers have developed a large number of different air quality indicators. This study focuses on air exchange efficiency ({\&}Epsilon;{\textless}inf{\textgreater}a{\textless}/inf{\textgreater}) and contaminant removal effectiveness ({\&}Epsilon;) as suitable indicators for use in design and on-site measurements. These two IAQ indicators were numerically studied and compared for five typical indoor spaces with different ventilation strategies and contaminant sources. Overall, more than fifty different simulations were performed. The results show that {\&}Epsilon;{\textless}inf{\textgreater}a{\textless}/inf{\textgreater} is appropriate for general evaluation of ventilation strategies for spaces with unknown contamination sources, while {\&}Epsilon; provides more informative results for removal of contaminants with known position and generation rates. Nevertheless, certain correlations between {\&}Epsilon;{\textless}inf{\textgreater}a{\textless}/inf{\textgreater} and {\&}Epsilon; exist. Based on the results of this study, designers could select an air quality indicator that is more appropriate for their particular application.},
