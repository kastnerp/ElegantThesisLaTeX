@article{Fisk1997,
abstract = {The air change effectiveness (ACE), an indicator of the indoor airflow pattern, was measured in twenty-six laboratory experiments. Ventilation air was supplied through inductiontype diffusers located in the ceiling and removed through a ceiling-mounted return grille. The tracer gas stepup measurement procedure was employed. In five of the experiments, pollutant removal efficiencies were also measured for simulated pollutant emissions from the floor covering and for simulated emissions from occupants. In experiments with heated supply air, supply air flow rates typical of the minimum supply flow rates of VAV ventilation systems, and 100{\%} outside air, the ACE ranged from 0.69 to 0.89. These results indicate that significant short circuiting of ventilation air between the supply air diffuser and return air grille does occur under these adverse conditions. Mechanical recirculation of air, such that the supply air contained approximately 50{\%} outside air, increased the ACE by about 0.05. When the supply air was cooled, the ACE ranged from 0.99 to 1.15, adding to existing evidence that short circuiting is rarely a problem when the building is being cooled. The pollutant removal efficiency for simulated pollutant emissions from the floor covering (PREfloor)was strongly correlated with ACE (R2 = 0.98) and the values of PREfloor were within approximately 0.1 of the values of ACE. The pollutant removal efficiency for simulated pollutant emissions by occupants varied between workstations and was not as well correlated with the ACE.},
author = {Fisk, W. J. and Faulkner, D. and Sullivan, D. and Bauman, F.},
doi = {10.1111/j.1600-0668.1997.t01-3-00007.x},
file = {Fisk et al. - 1997 - Air Change Effectiveness and Pollutant Removal Efficiency during Adverse Mixing Conditions.pdf:pdf},
issn = {0905-6947},
journal = {Indoor Air},
keywords = {air change effectiveness,buildings,efficiency,exposure,pollutant,ventilation},
number = {1},
pages = {55--63},
title = {{Air Change Effectiveness and Pollutant Removal Efficiency during Adverse Mixing Conditions}},
url = {http://doi.wiley.com/10.1111/j.1600-0668.1997.t01-3-00007.x},
volume = {7},
year = {1997}
}
