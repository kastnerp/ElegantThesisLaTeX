journal = {Energy and Buildings},
keywords = {Decentralized ventilation,Energy analysis,European climates,Hybrid ventilation,Natural ventilation},
pages = {424--433},
publisher = {Elsevier B.V.},
title = {{Energy analysis of a decentralized ventilation system compared with centralized ventilation systems in European climates: Based on review of analyses}},
url = {http://dx.doi.org/10.1016/j.enbuild.2015.11.044},
volume = {111},
year = {2016}
}
